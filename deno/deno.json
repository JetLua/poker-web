{
  "imports": {
    "hono": "jsr:@hono/hono",
    "axios": "npm:axios@latest",
    "mongo": "npm:mongodb@latest",
    "encoding": "jsr:@std/encoding",
    "presigner": "npm:@aws-sdk/s3-request-presigner@latest",
    "s3": "npm:@aws-sdk/client-s3@latest",
    "~/": "./src/"
  },
  "tasks": {
    "cache": "deno cache --reload src/app.ts",
    "deploy": "deployctl deploy --prod --org=astro",
    "dev": "deno run -A --env --unstable-http --unstable-kv --unstable-hmr src/app.ts"
  },
  "lint": {
    "rules": {
      "exclude": ["no-explicit-any", "ban-types"]
    }
  },
  "unstable": ["http", "kv"],
  "compilerOptions": {
    "lib": [
      "deno.window",
      "deno.unstable"
    ],
    "types": [],
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  },
  "deploy": {
    "project": "yake",
    "exclude": [
      ".env",
      "deno.lock",
      ".gitignore",
      ".prettierrc",
      ".editorconfig"
    ],
    "include": [],
    "entrypoint": "src/app.ts"
  }
}
